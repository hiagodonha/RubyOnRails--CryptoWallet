namespace :dev do
  desc "Configura o ambiente de desenvolvimento"
  task setup: :environment do
    if Rails.env.development?

      spinner = TTY::Spinner.new("[:spinner] Apagando BD...")
      spinner.auto_spin
      %x(rails db:drop)
      spinner.success('Conclído com sucesso!')

      spinner = TTY::Spinner.new("[:spinner] Criando BD...")
      spinner.auto_spin
      %x(rails db:create)
      spinner.success('Conclído com sucesso!')

      spinner = TTY::Spinner.new("[:spinner] Criando Migrate...")
      spinner.auto_spin
      %x(rails db:migrate)
      spinner.success('Conclído com sucesso!')

      %x(rails dev:add_mining_type)
      %x(rails dev:add_coins)

      # spinner = TTY::Spinner.new("[:spinner] Populando BD...")
      # spinner.auto_spin
      # %x(rails db:seed)
      # spinner.success('Conclído com sucesso!')

    else
      puts "Você não está em ambiente desenvolvimento!"
    end
  end

  desc "Cadastrando Moedas"
  task add_coins: :environment do

    spinner = TTY::Spinner.new("[:spinner] Cadastrando moedas ...")
    spinner.auto_spin
    coins = 
    [
      {
        description: "Bitcoin",
        acronym: "BTC",
        url_image: "",
        mining_type: MiningType.all.sample
      },
      {
        description: "Ethereum",
        acronym: "ETH",
        url_image: "",
        mining_type: MiningType.all.sample
      },
      {
        description: "Dash",
        acronym: "DASH",
        url_image: "",
        mining_type: MiningType.all.sample
      },
      {
        description: "Tether",
        acronym: "T",
        url_image: "data:image/png;base64,",
        mining_type: MiningType.all.sample
      }
    ]
    
    coins.each do |coin|
        Coin.find_or_create_by!(coin)
    end
    
    spinner.success("(Concluído!)")

  end

  desc "Cadastrando dos tipos de mineração"
  task add_mining_type: :environment do
    spinner = TTY::Spinner.new("[:spinner] Cadastrando Tipos de mineração...")
    spinner.auto_spin

    mining_types = [
      {description: "Proof of Work", acronym: "PoW"},
      {description: "Proof of Stake", acronym: "PoS"},
      {description: "Proof of Capacity", acronym: "PoC"}
    ]

    mining_types.each do |t|
      MiningType.find_or_create_by!(t)
    end

    spinner.success("(Concluído!)")

  end

end
